<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="progressbarColor" format="reference" />
    <attr name="showOnlySendIcon" format="boolean" />

    <declare-styleable name="EnterCodeWidget">
        <attr name="numBoxes" format="integer" />
        <attr name="isSecureText" format="boolean" />
        <attr name="errorText" format="string" />
    </declare-styleable>

    <declare-styleable name="FabIconSend">
        <attr name="showOnlySendIcon" />
    </declare-styleable>

    <declare-styleable name="SearchView">
        <attr name="hint" format="string" />
        <attr name="characterCount" format="integer" />
    </declare-styleable>

    <declare-styleable name="NumericKeyboardCustomView">
        <attr name="showBiometric" format="boolean" />
    </declare-styleable>

    <declare-styleable name="InputPanelWidget">
        <attr name="showEmojiIcon" format="boolean" />
        <attr name="showCameraIcon" format="boolean" />
        <attr name="showAttachmentIcon" format="boolean" />
        <attr name="showOnlySendIcon" />
        <attr name="showQuote" format="boolean" />
    </declare-styleable>

    <declare-styleable name="Quote">
        <attr name="isCancelable" format="boolean" />
        <attr name="isGoTo" format="boolean" />
        <attr name="isFromInputPanel" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AnimatedTwoVectorView">
        <attr name="firstAnimation" format="reference" />
        <attr name="secondAnimation" format="reference" />
    </declare-styleable>

    <declare-styleable name="AudioPlayerCustomView">
        <attr name="controlColors" format="reference" />
        <attr name="isEncryptedFile" format="boolean" />
        <attr name="absolutePath" format="string" />
        <attr name="audioId" format="integer" />
        <attr name="seekbarProgressBackgroundTint" format="reference" />
        <attr name="seekbarProgressTint" format="reference" />
        <attr name="seekbarThumbTint" format="reference" />
    </declare-styleable>

    <!--//Background-->
    <attr name="attrBackgroundColorPrimary" format="reference" />
    <attr name="attrBackgroundSectionColor" format="reference" />
    <attr name="attrBackgroundSectionOtherColor" format="reference" />
    <attr name="attrBackgroundColorBackground" format="reference" />
    <attr name="attrBackgroundColorBackgroundDialog" format="reference" />
    <attr name="attrColorTextButtonTintCamera" format="reference" />
    <attr name="attrBackgroundColorActionMode" format="reference" />
    <attr name="attrBackgroundBatchMessage" format="reference" />

    <!--//Tint-->
    <attr name="attrTintColorSecondaryDark" format="reference" />

    <!--TextColor-->
    <attr name="attrTextColorPrimaryTextColor" format="reference" />
    <attr name="attrTextColorSecondaryTextColor" format="reference" />
    <attr name="attrTextColorButtonTint" format="reference" />
    <attr name="attrTextColorRed" format="reference" />
    <attr name="attrTextColorWhite" format="reference" />
    <attr name="attrTextColorMessageAndTimeHome" format="reference" />
    <attr name="attrTextColorColorTextMyMessage" format="reference" />
    <attr name="attrTextColorColorTextIncomingMessage" format="reference" />
    <attr name="attrTextColorLinkTextColor" format="reference" />
    <attr name="attrTextColorColorError" format="reference" />
    <attr name="attrTextColorColorPrimary" format="reference" />
    <attr name="attrTextColorHint" format="reference" />
    <attr name="attrTextColorNameConversation" format="reference" />
    <attr name="attrTextColorNicknameConversation" format="reference" />
    <attr name="attrTextColorHintConversationInputPanel" format="reference" />
    <attr name="attrTextColorConversationInputPanel" format="reference" />
    <attr name="attrTextColorButtonsPreview" format="reference" />

    <!--ActionBarItem-->
    <attr name="attrActionBarItemBackground" format="reference" />

    <!--HintTextColor-->
    <attr name="attrHintTextColorSecondaryTextColor" format="reference" />

    <!--IconTint-->
    <attr name="attrIconTintWhite" format="reference" />

    <!--BackgroundTint-->
    <attr name="attrBackgroundTintColorSecondary" format="reference" />
    <attr name="attrBackgroundTintButtonTint" format="reference" />
    <attr name="attrBackgroundTintIconToolbar" format="reference" />

    <!--StrokeColor-->
    <attr name="attrStrokeColorButtonTint" format="reference" />
    <attr name="attrStrokeColorRed" format="reference" />

    <!--RippleColor-->
    <attr name="attrRippleColorRippleTint" format="reference" />
    <attr name="attrRippleColorRed" format="reference" />

    <!--boxStrokeColor-->
    <attr name="attrBoxStrokeColorTextInputLayoutBoxColor" format="reference" />

    <!--boxStrokeErrorColor-->
    <attr name="attrBoxStrokeErrorColorColorError" format="reference" />

    <!--errorTextColor-->
    <attr name="attrErrorTextColorColorError" format="reference" />

    <!--errorIconTint-->
    <attr name="attrErrorIconTintColorError" format="reference" />

    <!--fabIconColor-->
    <attr name="attrFabIconColor" format="reference" />

    <!--conversationIconColor-->
    <attr name="attrConversationIconColor" format="reference" />

    <!--Color-->
    <attr name="attrColorButtonTint" format="reference" />

    <!--TrackColor-->
    <attr name="attrColorTrackColor" format="reference" />

    <!--Separator-->
    <attr name="attrSeparatorColor" format="reference" />

    <!--Drawer-->
    <attr name="attrIconAndTextColorDrawer" format="reference" />

    <!-- Seekbar Progress Tint -->
    <attr name="attrSeekbarProgressTint" format="reference" />

    <!-- Seekbar Thumb Tint -->
    <attr name="attrSeekbarThumbTint" format="reference" />

    <declare-styleable name="CircleProgressBar">
        <attr name="min" format="integer" />
        <attr name="max" format="integer" />
        <attr name="progress" format="integer" />
        <attr name="progressbarColor" />
        <attr name="progressBarThickness" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ImageViewProgress">
        <attr name="progressbarColor" />
    </declare-styleable>

    <!-- Quote -->
    <attr name="attrBackgroundColorQuoteMyMessage" format="reference" />
    <attr name="attrBackgroundColorQuoteIncomingMessage" format="reference" />
    <attr name="attrIdentifierColorMyQuote" format="reference" />
    <attr name="attrIdentifierColorYourQuote" format="reference" />


    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API level.
         ?android:attr/buttonBarStyle is new as of API 11 so this is
         necessary to support previous API levels. -->
    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>
</resources>